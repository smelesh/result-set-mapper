{
    "name": "smelesh/result-set-mapper",
    "type": "library",
    "license": "MIT",
    "description": "Mapper for relational SQL result set.",
    "keywords": ["mapper", "result set", "database"],
    "authors": [
        {
            "name": "Sergey Melesh",
            "email": "sergei.melesh@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^8.0",
        "symfony/polyfill-php81": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.11"
    },
    "autoload": {
        "psr-4": {
            "Smelesh\\ResultSetMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smelesh\\ResultSetMapper\\Tests\\": "tests/"
        }
    }
}
